        "kyoko", "marc", "mei", "miusaki", "nanako", "nath", "nath_equipped", "natsumi", "nico", "poppo",
        "poppo_mix", "qp", "qp_dangerous", "repa", "saki", "sham", "sherry", "sora", "sora_military", "sora_swimsuit",
        "star_breaker", "suguri", "suguri_swimsuit", "suguri_winter", "sweet_breaker", "syura", "tomomo",
        "tomomo_casual", "tomomo_sweet_eater", "tsih", "yuki", "yuki_dangerous"
    ]
